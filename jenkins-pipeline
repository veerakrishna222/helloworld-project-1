pipeline {
agent {label 'master'}

 tools {
    jdk 'java'
    maven 'maven'
  }
stages{
   stage('git clone'){
       steps{
        git branch: 'main',
        url: 'https://github.com/chinni4321/helloworld-project-1.git'

       }
    }

   stage('maven build'){
       steps{
           sh 'mvn clean package'

       }
     }

 }
}
