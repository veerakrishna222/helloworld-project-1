pipeline {
agent {label 'master'}

 tools {
    jdk 'Java'
    maven 'Maven'
  }
stages{
   stage('Git clone'){
       steps{
        git branch: 'main',
        url: 'https://github.com/chinni4321/helloworld-project-1.git'

       }
    }

   stage('Maven build'){
       steps{
           sh 'mvn clean package'

       }
     }

 }
}
